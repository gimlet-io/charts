{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#redis",
  "type": "object",
  "title": "Containerized Redis",
  "description": "Install a Redis instance dedicated to your application",
  "properties": {
    "encryptedPassword": {
      "$id": "#/properties/encryptedPassword",
      "type": "string",
      "title": "Password"
    },
    "app": {
      "$id": "#/properties/app",
      "type": "string",
      "title": "App",
      "default": "{{ .APP }}"
    }
  }
}
